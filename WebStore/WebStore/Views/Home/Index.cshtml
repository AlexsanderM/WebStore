@model IEnumerable<EmployeeView>
@{
    ViewBag.Title = "Index";
}

@section topsection {
    @await Html.PartialAsync("Partial/_Slider")
}

<section>
    <div class="row">
        <div class="col-sm-3">
            @await Html.PartialAsync("Partial/_LeftSideBar")
        </div>
        <div class="col-sm-9 padding-right">
            @await Html.PartialAsync("Partial/_FeaturesItem")
            @await Html.PartialAsync("Partial/_CategoryTab")
            @await Html.PartialAsync("Partial/_RecommendedItem")
        </div>
    </div>
</section>
