@model IEnumerable<EmployeeView>
@{
    ViewBag.Title = "Index";
}

<p>Test</p>

<table>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.FirstName</td>
            <td>@item.SurName</td>
            <td>@item.Patronymic</td>
            <td>@Html.ActionLink("Детали", "Details", new { id = item.Id })</td>
        </tr>
    }
</table>

